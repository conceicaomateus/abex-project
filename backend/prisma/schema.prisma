generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         String   @id
  name       String?
  username   String   @unique
  email      String   @unique
  password   String
  active     Boolean  @default(true)
  created_at DateTime @default(now())
  updated_at DateTime
}
